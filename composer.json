{
    "name": "touchsms/touchsms",
    "type": "library",
    "description": "An easy-to-use PHP Class for accessing touchSMS API.",
    "keywords": ["touchsms", "sms", "sms gateway", "mobile", "cellphone"],
    "homepage": "https://github.com/touchsms/touchsms-PHP-API",
    "license": "MIT",
    "authors": [
        {
            "name": "touchSMS",
            "email": "support@touchsms.com.au",
            "homepage": "http://touchsms.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "TouchSMS\\TouchSMS\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
